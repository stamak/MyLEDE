
config global
	option anon_swap '0'
	option anon_mount '0'
	option auto_swap '1'
	option auto_mount '1'
	option delay_root '5'
	option check_fs '0'

config mount
	option target '/storage/1'
	option uuid '4e8b2be3-ccb2-4953-92b5-fc34ee2713e7'
	option fstype 'ext4'
	option options 'rw,sync'
	option enabled '1'

config mount
	option target '/storage/3'
	option uuid '688e1e3b-8b76-49d5-bb20-83ef07960270'
	option fstype 'ext4'
	option options 'rw,sync'
	option enabled '1'
